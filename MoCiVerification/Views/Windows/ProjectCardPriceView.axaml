        </suki:SukiWindow.LogoContent>
        <Grid Margin="10">
        <suki:GlassCard Margin="0,10,0,30" Width="400"
                        animations:SquishyOverExtensions.Enable="True"
                        animations:SquishyOverExtensions.Intensity="0.05"
                        animations:SquishyOverExtensions.SquishDepth="0.05">
            <suki:BusyArea IsBusy="{Binding IsChanging}">
                <ScrollViewer>
                    <StackPanel Orientation="Horizontal" Spacing="30" >
                        <StackPanel>
                        <TextBlock Margin="6,0,0,3"
                                           FontWeight="DemiBold" TextWrapping="Wrap"
                                           Foreground="{DynamicResource SukiLowText}"
                                           Text="时卡标准价格" />
                        <TextBox suki:TextBoxExtensions.Prefix="{icon:MaterialIconExt Kind=CardText}"
                                 Classes="clearButton" TextWrapping="Wrap"
                                 Text="{Binding StandCard1}"
                                 Watermark="Input your price"/>
                        <TextBlock Margin="6,10,0,3"
                                   FontWeight="DemiBold" TextWrapping="Wrap"
                                   Foreground="{DynamicResource SukiLowText}"
                                   Text="天卡标准价格" />
                        <TextBox suki:TextBoxExtensions.Prefix="{icon:MaterialIconExt Kind=CardText}"
                                 Classes="clearButton" TextWrapping="Wrap"
                                 Text="{Binding StandCard2}"
                                 Watermark="Input your price"/>
                        <TextBlock Margin="6,10,0,3"
                                   FontWeight="DemiBold" TextWrapping="Wrap"
                                   Foreground="{DynamicResource SukiLowText}"
                                   Text="周卡标准价格" />
                        <TextBox suki:TextBoxExtensions.Prefix="{icon:MaterialIconExt Kind=CardText}"
                                 Classes="clearButton" TextWrapping="Wrap"
                                 Text="{Binding StandCard3}"
                                 Watermark="Input your price"/>
                        <TextBlock Margin="6,10,0,3"
                                   FontWeight="DemiBold" TextWrapping="Wrap"
                                   Foreground="{DynamicResource SukiLowText}"
                                   Text="月卡标准价格" />
                        <TextBox suki:TextBoxExtensions.Prefix="{icon:MaterialIconExt Kind=CardText}"
                                 Classes="clearButton" TextWrapping="Wrap"
                                 Text="{Binding StandCard4}"
                                 Watermark="Input your price"/>
                        <TextBlock Margin="6,10,0,3"
                                   FontWeight="DemiBold" TextWrapping="Wrap"
                                   Foreground="{DynamicResource SukiLowText}"
                                   Text="季卡标准价格" />
                        <TextBox suki:TextBoxExtensions.Prefix="{icon:MaterialIconExt Kind=CardText}"
                                 Classes="clearButton" TextWrapping="Wrap"
                                 Text="{Binding StandCard5}"
                                 Watermark="Input your price"/>
                        <TextBlock Margin="6,10,0,3"
                                   FontWeight="DemiBold" TextWrapping="Wrap"
                                   Foreground="{DynamicResource SukiLowText}"
                                   Text="半年卡标准价格" />
                        <TextBox suki:TextBoxExtensions.Prefix="{icon:MaterialIconExt Kind=CardText}"
                                 Classes="clearButton" TextWrapping="Wrap"
                                 Text="{Binding StandCard6}"
                                 Watermark="Input your price"/>
                        <TextBlock Margin="6,10,0,3"
                                   FontWeight="DemiBold" TextWrapping="Wrap"
                                   Foreground="{DynamicResource SukiLowText}"
                                   Text="年卡标准价格" />
                        <TextBox suki:TextBoxExtensions.Prefix="{icon:MaterialIconExt Kind=CardText}"
                                 Classes="clearButton" TextWrapping="Wrap"
                                 Text="{Binding StandCard7}"
                                 Watermark="Input your price"/>
                        <TextBlock Margin="6,10,0,3"
                                   FontWeight="DemiBold" TextWrapping="Wrap"
                                   Foreground="{DynamicResource SukiLowText}"
                                   Text="永久卡标准价格" />
                        <TextBox suki:TextBoxExtensions.Prefix="{icon:MaterialIconExt Kind=CardText}"
                                 Classes="clearButton" TextWrapping="Wrap"
                                 Text="{Binding StandCard8}"
                                 Watermark="Input your price"/>
                    </StackPanel>
                        <StackPanel>
                        <TextBlock Margin="6,0,0,3"
                                           FontWeight="DemiBold" TextWrapping="Wrap"
                                           Foreground="{DynamicResource SukiLowText}"
                                           Text="时卡代理价格" />
                        <TextBox suki:TextBoxExtensions.Prefix="{icon:MaterialIconExt Kind=CardText}"
                                 Classes="clearButton" TextWrapping="Wrap"
                                 Text="{Binding ProxyCard1}"
                                 Watermark="Input your price"/>
                        <TextBlock Margin="6,10,0,3"
                                   FontWeight="DemiBold" TextWrapping="Wrap"
                                   Foreground="{DynamicResource SukiLowText}"
                                   Text="天卡代理价格" />
                        <TextBox suki:TextBoxExtensions.Prefix="{icon:MaterialIconExt Kind=CardText}"
                                 Classes="clearButton" TextWrapping="Wrap"
                                 Text="{Binding ProxyCard2}"
                                 Watermark="Input your price"/>
                        <TextBlock Margin="6,10,0,3"
                                   FontWeight="DemiBold" TextWrapping="Wrap"
                                   Foreground="{DynamicResource SukiLowText}"
                                   Text="周卡代理价格" />
                        <TextBox suki:TextBoxExtensions.Prefix="{icon:MaterialIconExt Kind=CardText}"
                                 Classes="clearButton" TextWrapping="Wrap"
                                 Text="{Binding ProxyCard3}"
                                 Watermark="Input your price"/>
                        <TextBlock Margin="6,10,0,3"
                                   FontWeight="DemiBold" TextWrapping="Wrap"
                                   Foreground="{DynamicResource SukiLowText}"
                                   Text="月卡代理价格" />
                        <TextBox suki:TextBoxExtensions.Prefix="{icon:MaterialIconExt Kind=CardText}"
                                 Classes="clearButton" TextWrapping="Wrap"
                                 Text="{Binding ProxyCard4}"
                                 Watermark="Input your price"/>
                        <TextBlock Margin="6,10,0,3"
                                   FontWeight="DemiBold" TextWrapping="Wrap"
                                   Foreground="{DynamicResource SukiLowText}"
                                   Text="季卡代理价格" />
                        <TextBox suki:TextBoxExtensions.Prefix="{icon:MaterialIconExt Kind=CardText}"
                                 Classes="clearButton" TextWrapping="Wrap"
                                 Text="{Binding ProxyCard5}"
                                 Watermark="Input your price"/>
                        <TextBlock Margin="6,10,0,3"
                                   FontWeight="DemiBold" TextWrapping="Wrap"
                                   Foreground="{DynamicResource SukiLowText}"
                                   Text="半年卡代理价格" />
                        <TextBox suki:TextBoxExtensions.Prefix="{icon:MaterialIconExt Kind=CardText}"
                                 Classes="clearButton" TextWrapping="Wrap"
                                 Text="{Binding ProxyCard6}"
                                 Watermark="Input your price"/>
                        <TextBlock Margin="6,10,0,3"
                                   FontWeight="DemiBold" TextWrapping="Wrap"
                                   Foreground="{DynamicResource SukiLowText}"
                                   Text="年卡代理价格" />
                        <TextBox suki:TextBoxExtensions.Prefix="{icon:MaterialIconExt Kind=CardText}"
                                 Classes="clearButton" TextWrapping="Wrap"
                                 Text="{Binding ProxyCard7}"
                                 Watermark="Input your price"/>
                        <TextBlock Margin="6,10,0,3"
                                   FontWeight="DemiBold" TextWrapping="Wrap"
                                   Foreground="{DynamicResource SukiLowText}"
                                   Text="永久卡代理价格" />
                        <TextBox suki:TextBoxExtensions.Prefix="{icon:MaterialIconExt Kind=CardText}"
                                 Classes="clearButton" TextWrapping="Wrap"
                                 Text="{Binding ProxyCard8}"
                                 Watermark="Input your price"/>
                    </StackPanel>
                    </StackPanel>
                </ScrollViewer>
            </suki:BusyArea>
        </suki:GlassCard>
        <Button Width="100"
                Height="40"
                Margin="0,0,10,10"
                HorizontalAlignment="Right"
                VerticalAlignment="Bottom"
                Classes="Flat" FontWeight="DemiBold"
                suki:ButtonExtensions.ShowProgress="{Binding IsChanging}"
                Command="{Binding ChangeProjectCardPrice}" 
                Content="保存配置" />
                
    </Grid>
        
        
        
        
        
        
</suki:SukiWindow>
